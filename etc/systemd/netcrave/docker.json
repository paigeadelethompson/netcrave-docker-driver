{
  "bip": "203.0.113.1/25",
  "bridge": "docker",
  "data-root": "/srv/docker/data",
  "debug": true,
  "default-address-pools": [
    {
      "base": "203.0.113.128/25",
      "size": 29
    }],
  "default-shm-size": "64K",
  "default-ulimits": {
    "nofile": {
      "Hard": 64,
      "Name": "nofile",
      "Soft": 64
    }
  },
  "dns": ["2001:db8:aaaa:aaae:192:0:0:18", "192.0.0.18"],
  "dns-search": [".local"],
  "exec-root": "/srv/docker/state",
  "fixed-cidr-v6": "fc01::/64",
  "group": "docker",
  "hosts": ["/run/docker/_sock"],
  "proxies": {
    "http-proxy": "http://proxy.local:3128",
    "https-proxy": "https://proxy.local:443",
    "no-proxy": "*.local",
  },
  "ip-forward": true,
  "ip-masq": false,
  "iptables": false,
  "ip6tables": false,
  "ipv6": true,
  "live-restore": false,
  "log-driver": "fluentd",
  "log-opts": {
    "fluentd-address": "fluentd.local:24224"
  },
  "mtu": 9000,
  "oom-score-adjust": 0,
  "pidfile": "/srv/docker/docker.pid",
  "selinux-enabled": true,
  "shutdown-timeout": 64,
  "tls": true,
  "tlscacert": "/etc/netcrave/ssl/ca.pem",
  "tlscert": "/etc/netcrave/ssl/docker.pem",
  "tlskey": "/etc/netcrave/ssl/docker.key",
  "tlsverify": true,
  "userns-remap": "default"
}
